    <style name="TextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu">
        <item name="boxStrokeColor">@color/colorSubTitleTextLite</item>
        <item name="android:textColorHint">@color/colorSubTitleTextLite</item>
        <item name="hintTextColor">@color/colorSubTitleTextLite</item>
        <item name="boxBackgroundColor">@color/background_button</item>
        <item name="boxBackgroundMode">none</item>
        <item name="android:state_focused">true</item>
        <item name="android:state_hovered">false</item>
        <item name="android:state_enabled">true</item>
        <item name="android:background">@drawable/background_curve_rectanguler</item>
        <item name="android:backgroundTint">@color/background_button</item>
        <item name="boxStrokeWidth">0dp</item>
    </style>

