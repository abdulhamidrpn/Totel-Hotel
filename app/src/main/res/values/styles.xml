    </style>
    <style name="icon_medium_style">
        <item name="android:layout_width">@dimen/_46sdp</item>
        <item name="android:layout_height">@dimen/_46sdp</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:tint">@color/colorOnBackground</item>
    </style>

    <style name="button_icon_style">
        <item name="android:layout_width">@dimen/_30sdp</item>
        <item name="android:layout_height">@dimen/_30sdp</item>
        <item name="android:tint">@color/colorOnBackground</item>
