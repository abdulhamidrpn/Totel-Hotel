        <item name="android:elevation">@dimen/_6sdp</item>
        <item name="android:padding">@dimen/_10sdp</item>
        <item name="android:scaleType">centerCrop</item>
    </style>

    <style name="profile_style" parent="">
        <item name="android:layout_width">@dimen/_52sdp</item>
        <item name="android:layout_height">@dimen/_52sdp</item>
        <item name="shapeAppearance">@style/ShapeAppearance.App.CircleImageView</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="srcCompat">@drawable/potrait_man</item>
    </style>

    <style name="button_style" parent="">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/background_curve_rectanguler</item>
        <item name="android:elevation">@dimen/_8sdp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="button_stroke_style" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:elevation">@dimen/_8sdp</item>
        <item name="shapeAppearanceOverlay">@style/ShapeAppearance.App.Button.Round</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/colorTitleText</item>
    </style>

    <style name="thumbnail_style" parent="">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_140sdp</item>
        <item name="android:background">@drawable/background_curve_rectanguler</item>
        <item name="android:elevation">@dimen/_4sdp</item>
        <item name="cardCornerRadius">@dimen/_12sdp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:foreground">?android:selectableItemBackground</item>
    </style>
