<resources>


    <style name="OverScrollColor">
        <item name="android:colorPrimary">@color/colorPrimary</item>
    </style>

    <style name="TabLayoutTextAppearance" parent="Widget.MaterialComponents.TabLayout">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/colorTitleText</item>
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="BottomSheetDialogShapeAppearance">
        <item name="cornerSizeTopLeft">@dimen/_26sdp</item>
        <item name="cornerSizeTopRight">@dimen/_26sdp</item>
    </style>

    <style name="CustomBottomSheetDialogTheme" parent="@style/ThemeOverlay.Material3.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@style/CustomBottomSheetAnimation</item>
    </style>

    <style name="CustomBottomSheetAnimation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/slide_in_bottom</item>
        <item name="android:windowExitAnimation">@anim/slide_out_bottom</item>
    </style>



    <style name="ShapeAppearance.App.CircleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearance.App.Button.Round" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearance.App.RoundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/_8sdp</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:layout_marginBottom">@dimen/_8sdp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="textWithBackground">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorTitleText</item>
        <item name="android:textSize">@dimen/_18ssp</item>
        <item name="android:paddingStart">@dimen/_12sdp</item>
        <item name="android:paddingEnd">@dimen/_12sdp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">@dimen/_8sdp</item>
    </style>
    <style name="titleText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/colorTitleText</item>
        <item name="android:textSize">@dimen/_18ssp</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">@dimen/_8sdp</item>
    </style>

