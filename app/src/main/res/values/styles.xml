        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/background_button</item>
        <item name="android:textColor">@color/colorTitleText</item>
        <item name="android:textColorHint">@color/colorSubTitleTextLite</item>
        <item name="android:fontFamily">@font/sf_pro_text_regular</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:paddingStart">@dimen/_12sdp</item>
        <item name="android:paddingEnd">@dimen/_12sdp</item>
